<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('../partials/head'); %>
</head>
<body class="container">

<header>
  <%- include('../partials/header'); %>
</header>

<main>
    <div id="booking" class="section">
        <div class="section-center">
            <div class="container">
                <div class="row">
                    <div class="booking-form">
                        <div class="form-header">
                            <h1>Login</h1>
                        </div>
                        <form action="/login" method="POST">
                            <div class="form-group"> <input class="form-control" type="UserName" name="username" id="username" placeholder="UserName" required> <span class="form-label">User Name</span> </div>
                            <div class="form-group"> <input class="form-control" type="Password" name="password" id="password" placeholder="Password" required> <span class="form-label">Password</span> </div>
                            <div class="form-btn"> <button class="submit-btn">Sign In</button> </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</main>

<footer>
  <%- include('../partials/footer'); %>
</footer>

</body>
</html>